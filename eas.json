{
    "cli": {
        "version": ">= 7.3.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "channel": "development",
            "ios": { "simulator": true },
            "env": {
                "APP_VARIANT": "development"
            }
        },
        "production": {
            "channel": "production",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "APP_VARIANT": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
