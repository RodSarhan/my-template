{
  "name": "simple-reminders",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "test": "jest --watchAll",
    "lint": "eslint .",
    "test-types": "tsc --noEmit",
    "// Dev": "",
    "prebuild-dev": "APP_VARIANT=development expo prebuild --clean",
    "start-dev": "APP_VARIANT=development expo start --dev-client",
    "ios-dev": "APP_VARIANT=development expo run:ios",
    "android-dev": "APP_VARIANT=development expo run:android",
    "web-dev": "APP_VARIANT=development expo start --web --dev-client",
    "build-dev-ios": "APP_VARIANT=development eas build --platform ios --local --profile development",
    "build-dev-android": "APP_VARIANT=development eas build --platform android --local --profile development",
    "// Prod": "",
    "prebuild-production": "APP_VARIANT=production expo prebuild --clean",
    "build-production-android": "APP_VARIANT=production eas build --platform android --local --profile production",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "printWidth": 100
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^6.0.2",
    "@tanstack/query-sync-storage-persister": "^5.28.6",
    "@tanstack/react-query": "^5.28.6",
    "@tanstack/react-query-persist-client": "^5.28.6",
    "axios": "^1.6.8",
    "expo": "~50.0.8",
    "expo-dev-client": "~3.3.9",
    "expo-font": "~11.10.3",
    "expo-image": "~1.10.6",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "i18next": "^23.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.4",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-theme-switch-animation": "^0.6.0",
    "react-native-unistyles": "^2.5.0",
    "react-native-web": "~0.19.6",
    "remeda": "^1.56.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.2",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
