{
  "name": "my-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "test": "jest --watchAll",
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "test-types": "tsc --noEmit",
    "postinstall": "patch-package && setup-skia-web",
    "// Dev": "",
    "prebuild-dev": "APP_VARIANT=development expo prebuild --clean",
    "start-dev": "APP_VARIANT=development expo start --dev-client",
    "ios-dev": "APP_VARIANT=development expo run:ios",
    "android-dev": "APP_VARIANT=development expo run:android",
    "web-dev": "APP_VARIANT=development expo start --web --dev-client",
    "build-dev-ios": "APP_VARIANT=development eas build --platform ios --local --profile development",
    "build-dev-android": "APP_VARIANT=development eas build --platform android --local --profile development",
    "open-url-ios": "wrapper() { npx uri-scheme open mytemplate://$1 --ios; }; wrapper ",
    "// Prod": "",
    "prebuild-production": "APP_VARIANT=production expo prebuild --clean",
    "build-production-android": "APP_VARIANT=production eas build --platform android --local --profile production",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 4,
    "printWidth": 100
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@marceloterreiro/flash-calendar": "^0.0.8",
    "@react-native-community/netinfo": "11.1.0",
    "@react-navigation/native": "^6.0.2",
    "@shopify/flash-list": "1.6.3",
    "@shopify/react-native-skia": "^1.0.4",
    "@tanstack/query-sync-storage-persister": "^5.28.6",
    "@tanstack/react-query": "^5.28.6",
    "@tanstack/react-query-persist-client": "^5.28.6",
    "axios": "^1.6.8",
    "chance": "^1.1.11",
    "dayjs": "^1.11.10",
    "expo": "^50.0.14",
    "expo-application": "~5.8.3",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.11",
    "expo-file-system": "~16.0.8",
    "expo-font": "~11.10.3",
    "expo-image": "~1.10.6",
    "expo-linking": "~6.2.2",
    "expo-navigation-bar": "~2.8.1",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-store-review": "~6.8.3",
    "expo-system-ui": "~2.9.3",
    "expo-updates": "~0.24.12",
    "expo-web-browser": "~12.8.2",
    "i18next": "^23.10.1",
    "immer": "^10.0.4",
    "numeral": "^2.0.6",
    "pokeapi-typescript": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-skottie": "^2.1.0",
    "react-native-theme-switch-animation": "^0.6.0",
    "react-native-unistyles": "^2.5.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.6.4",
    "remeda": "^1.56.0",
    "semver": "^7.6.0",
    "ts-pattern": "^5.0.8",
    "use-context-selector": "^1.4.4",
    "voca": "^1.4.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/eslint-config": "^0.75.0-main",
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@types/chance": "^1.1.6",
    "@types/numeral": "^2.0.5",
    "@types/react": "~18.2.45",
    "@types/voca": "^1.4.5",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.4",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
